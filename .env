# Database Configuration
# URL for the database connection
DATASOURCE_URL=

# Username for the database connection
DATASOURCE_USERNAME=

# Password for the database connection
DATASOURCE_PASSWORD=

# Logging Configuration
# Root log level (e.g., DEBUG, INFO, WARN, ERROR)
LOG_LEVEL_ROOT=

# Log level for Hibernate SQL statements
LOG_LEVEL_HIBERNATE_SQL=

# Log level for 3OM-specific logs
LOG_LEVEL_3OM=

# Server Configuration
# Port on which the server will run
SERVER_PORT=

# Enable or disable response compression (true/false)
SERVER_ENABLE_COMPRESSION=

# Comma-separated list of MIME types to compress
SERVER_MIME_TYPES=

# Minimum response size in bytes to enable compression
SERVER_MIN_RESPONSE_SIZE=